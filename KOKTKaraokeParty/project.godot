; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="KOKT Karaoke Party"
run/main_scene="res://root.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="KOKTKaraokeParty"
config/features=PackedStringArray("4.3", "C#", "Mobile")
boot_splash/show_image=false
config/icon="res://Graphics/Kadda OK 2025 1024.png"
config/macos_native_icon="res://Graphics/Kadda OK 2025 icon.icns"
config/windows_native_icon="res://Graphics/Kadda OK 2025 icon.ico"

[debug]

file_logging/max_log_files=10

[display]

window/size/viewport_width=1250
window/size/viewport_height=700
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="KOKTKaraokeParty"
project/solution_directory="../"

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
